                <ul class="dropdown-menu">
                    <li class="<%= typeof page !== 'undefined' && page === 'company' ? 'active' : '' %>"><a href="/seniors/indexcompany">Service Provider</a></li>
                    <li class="<%= typeof page !== 'undefined' && page === 'entertainment' ? 'active' : '' %>"><a href="/seniors/indexentertainment">Entertainment Provider</a></li>
                    <li class="<%= typeof page !== 'undefined' && page === 'allcombined' ? 'active' : '' %>"><a href="/seniors/allcombined">All Talents</a></li>
                </ul>
                
                
              </ul>
              

              
              
              
            </div>
        </div>
    </nav>
    

   <div class="container">
        <% if(error && error.length > 0) { %>
            <div class="alert alert-danger" role="alert">
                <%= error %>
            </div>
        <% } %>
        <% if(success && success.length > 0) { %>
            <div class="alert alert-success" role="alert">
                <%= success %>
            </div>
        <% } %>
    </div>
    
