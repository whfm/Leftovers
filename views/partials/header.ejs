        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                  <span class="sr-only">Toggle navigation</span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                </button>
    
                <a class="navbar-brand" href="/"><span class="fa fa-hand-holding-heart" aria-hidden="true" style="font-size: 100%;"></span>&nbsp&nbsp&nbsp&nbspO-Ra</a>
            </div>
            <div id="navbar" class="collapse navbar-collapse">

              <ul class="nav navbar-nav navbar-right">
                    <li class="<%= typeof page !== 'undefined' && page === 'aboutus' ? 'active' : '' %>"><a href="/aboutus"><i class="fas fa-info-circle"></i> About Us</a></li>
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <li class="<%= typeof page !== 'undefined' && page === 'aboutus' ? 'active' : '' %>"><a href="/networking"><i class="fas fa-comments"></i> Networking</a></li>
                </ul>
              <ul class="nav navbar-nav navbar-right">
                    <li class="<%= typeof page !== 'undefined' && page === 'aboutus' ? 'active' : '' %>"><a href="#"><i class="fas fa-volume-up"></i> Audio ON</a></li>
                </ul>
            
              <ul class="nav navbar-nav navbar-right">

                  
                  
                  <% if(!currentUser){ %>
                      <li class="<%= typeof page !== 'undefined' && page === 'login' ? 'active' : '' %>"><a href="/login"><i class="fas fa-sign-in-alt"></i> Login</a></li>
                      <li class="<%= typeof page !== 'undefined' && page === 'register' ? 'active' : '' %>"><a href="/register"><i class="fas fa-user-plus"></i> Sign Up</a></li>
                  <% } else { %>
                  
                  <ul class="nav navbar-nav navbar-right">
                <li class="dropdown">
                  <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><i class="fas fa-user"></i> Signed in as: <%= currentUser.username %> <span class="caret"></span></a>
                  <ul class="dropdown-menu">
                    <li><a href="/users/<%= currentUser.id %>">View my profile</a></li>
                    <li><a href="/users/<%= currentUser.id %>/edit">Edit my profile</a></li>
                    <li role="separator" class="divider"></li>
                    <li><a href="/logout">Logout</a></li>
                  </ul>
                </li>
          </ul>

                      
                  <% } %>
              </ul>
              

              
              <ul class="nav navbar-nav navbar-right">
                <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">Talents
                        <span class="caret"></span></a>
                <ul class="dropdown-menu">
                    <li class="<%= typeof page !== 'undefined' && page === 'seniors' ? 'active' : '' %>"><a href="/seniors">Experienced Talents</a></li>
                    <li class="<%= typeof page !== 'undefined' && page === 'young' ? 'active' : '' %>"><a href="/seniors/indexyoung">Young Talents</a></li>
                    <li class="<%= typeof page !== 'undefined' && page === 'allcombined' ? 'active' : '' %>"><a href="/seniors/allcombined">All Talents</a></li>
                </ul>
              </ul>
              
              <ul class="nav navbar-nav navbar-right">
